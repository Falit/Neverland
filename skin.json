{
	"name": "Neverland",
	"author": [
		"Ingo Malchow",
		"Gianluca Rigoletti"
	],
	"url": "https://github.com/WikiToLearn/Neverland",
	"descriptionmsg": "neverland-desc",
	"namemsg": "skinname-neverland",
	"license-name": "MIT",
	"type": "skin",
	"ValidSkinNames": {
		"neverland": "Neverland"
	},
	"MessagesDirs": {
		"Neverland": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"SkinNeverland": "Neverland.php"
	},
	"ResourceModules": {
		"skin.neverland": {
			"position": "top",
			"styles": {
					"css/bootstrap.css": { "media": "screen" },
					"css/bootstrap-social.css": { "media": "screen" },
					"css/font-awesome-4.4.0/css/font-awesome.css": { "media": "screen" }
			}
		},
		"skin.neverland.js": {
			"scripts": [
				"js/neverland.js"
			],
			"position": "bottom",
			"dependencies": [
				"skin.bootstrap.js",
				"jquery.throttle-debounce"
			]
		},
		"skin.bootstrap.js": {
			"scripts": [
				"js/bootstrap.min.js"
			],
			"position": "bottom"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Neverland"
	},
	"config": {
		"NeverlandUseSimpleSearch": true,
		"NeverlandUseIconWatch": true
	},
	"manifest_version": 1
}
